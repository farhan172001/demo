Properties props = new Properties();
props.put(MQC.TRANSPORT_PROPERTY, MQC.TRANSPORT_MQSERIES_CLIENT);
props.put(MQC.HOST_NAME_PROPERTY, mqHost);
props.put(MQC.PORT_PROPERTY, Integer.parseInt(mqPort));
props.put(MQC.CHANNEL_PROPERTY, mqChannel);

if (mqUsername != null) {
    props.put(MQC.USER_ID_PROPERTY, mqUsername);
}
if (mqPassword != null) {
    props.put(MQC.PASSWORD_PROPERTY, mqPassword);
}

mqQueueManager = new MQQueueManager(mqManagerName, props);





Hashtable<String, Object> properties = new Hashtable<>();
properties.put(MQConstants.HOST_NAME_PROPERTY, mqHost);
properties.put(MQConstants.PORT_PROPERTY, Integer.parseInt(mqPort));
properties.put(MQConstants.CHANNEL_PROPERTY, mqChannel);
properties.put(MQConstants.USER_ID_PROPERTY, mqUsername);
properties.put(MQConstants.PASSWORD_PROPERTY, mqPassword);

// Add this - important for client connections
properties.put(MQConstants.TRANSPORT_PROPERTY, MQConstants.TRANSPORT_MQSERIES_CLIENT);

mqQueueManager = new MQQueueManager(mqManagerName, properties);
